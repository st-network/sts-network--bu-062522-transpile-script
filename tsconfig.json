{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true
    },
    "exclude": [
      "node_modules",
      "src"
    ],
    "include": [
      "next-env.d.ts",
      "pages/*.{tsx,ts,jsx,js}",
      "/*.build.sh",
      "api/users/[is].js",
      "routes/*.js",
      "network/data/*.json",
      "pages/index.tsx", "pages/[projects]/namespace.tsx"  ]
  }